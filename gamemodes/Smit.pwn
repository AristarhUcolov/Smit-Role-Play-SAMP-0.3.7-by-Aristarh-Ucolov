{
string[0] = EOS;
format(string,126, "* %s достал веревку и завязал %s на узел.", PlayerInfo[playerid][pNames] ,PlayerInfo[params[0]][pNames]);
ProxDetector(30.0, playerid, string, 0xDD90FFFF,0xDD90FFFF,0xDD90FFFF,0xDD90FFFF,0xDD90FFFF);
TogglePlayerControllable(params[0], false);
PlayerTied{params[0]} = true;
return 1;
}
if(GetPlayerState(playerid)!=PLAYER_STATE_DRIVER) return SendClientMessage(playerid,-1,""#NET"Вы не водитель!");
new car = VehicleEnters[playerid];
if(!IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) != 2 && !IsPlayerInVehicle(params[0], car)) return SendClientMessage(playerid, -1, ""#NET"Человек не в машине!");
string[0] = EOS;
format(string,126, "* %s достал веревку и завязал %s на узел.", PlayerInfo[playerid][pNames] ,PlayerInfo[params[0]][pNames]);
ProxDetector(30.0, playerid, string, 0xDD90FFFF,0xDD90FFFF,0xDD90FFFF,0xDD90FFFF,0xDD90FFFF);
TogglePlayerControllable(params[0], false);
PlayerTied{params[0]} = true;
return 1;
}
