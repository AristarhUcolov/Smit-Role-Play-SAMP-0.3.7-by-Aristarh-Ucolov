PlayerInfo[gplayerid][pLeader] = 0;
PlayerInfo[gplayerid][pRank] = 0;
PlayerInfo[gplayerid][vig] = 0;

mysql_format(connects, string, 164, "UPDATE `accounts` SET `pMember` = '0', `pRank` = '0', `pLeader` = '0', `vig` = '0' WHERE `id` = '%d'", PlayerInfo[gplayerid][pID]);
mysql_empty(connects, string);

SetPlayerInterior(gplayerid, 0);
SendClientMessage(gplayerid, COLOR_REDD, "* Лидер добавил Вас в черный список, Вы уволены и не сможете вступить в его организацию!");
SCMF(gplayerid,COLOR_REDD,"* Причина добавления: %s", reason_chs);
return 1;
}
else if(status == 1 && !rows) return SendClientMessage(playerid, -1, ""#NET"Игрок не состоит в черном списке!");
else if(status == 1 && rows)
{
mysql_format(connects,string,144,"DELETE FROM `blocks` WHERE `blockName` = '%e' AND `blockFraction` = '%i'",PlayerInfo[gplayerid][pNames],PlayerInfo[playerid][pMember]);
mysql_empty(connects,string);
SendClientMessage(playerid, -1, "* Игрок успешно удален из черного списка Вашей фракции!");
return 1;
}
else if(status==3 && rows) return SendClientMessage(playerid, -1, ""#NET"Игрок находится в черном списке фракции!");
else if(status==2)
{
format(string,sizeof(string), ""WHITE"Номер\t\tИмя\t\tПричина добавления");
new NAME[24],REASON[64];
for(new i; i < rows; i ++)
{
cache_get_field_content(i,"blockName",NAME,connects,sizeof(NAME)),cache_get_field_content(i,"blockReason",REASON,connects,sizeof(REASON));
if(!strcmp(NAME, "None")) format(string,sizeof(string), "%s\n\n%i - ПУСТО\n",string,i+1);
else format(string,sizeof(string), "%s\n%i.\t\t%s\t\t%s\n",string,i+1,NAME,REASON);
}
ShowPlayerDialog(playerid, 0000, DIALOG_STYLE_MSGBOX, ""SERVER"Черный список фракции!",string,"Принять", "");
return 1;
}
else if(status==3)
{
format(string,200,""WHITE"%s предлагает Вам вступить в организацию: "SERVER"%s"WHITE"\n\nНажмите Да для согласия.\nНажмите Нет для отказа!",PlayerInfo[playerid][pNames],FractionInfo[PlayerInfo[playerid][pMember]][fName]);
SPD(gplayerid, 1147, DIALOG_STYLE_MSGBOX, ""SERVER"Предложение",string,"Да","Нет");
SCMF(playerid, -1, "Вы предложили "SERVER"%s"WHITE" вступить в организацию "SERVER"%s",PlayerInfo[gplayerid][pNames],FractionInfo[PlayerInfo[playerid][pMember]][fName]);
SetPVarInt(gplayerid, "pOfferFractionid", PlayerInfo[playerid][pMember]);
SetPVarInt(gplayerid,"Player",playerid);
SetPVarInt(playerid,"actinplayerid",gplayerid);
return 1;
}
return 1;
}

