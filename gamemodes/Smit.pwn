publics: OffWarn(playerid,tmp[],reason[])
{
new rows,fields;
cache_get_data(rows,fields,connects);
if(!rows) return SCMF(playerid, COLOR_WHITE, ""#NET"Аккаунт %s не найден в базе данных.",tmp);
if ( GetPlayerID(tmp) != -1 ) return SendClientMessage(playerid, -1, ""#NET"Игрок онлайн");
new pl_warns = cache_get_field_content_int(0, "pWarns", connects);
if((pl_warns+1) < 3)
{
mysql_format(connects, string, 264, "UPDATE `accounts` SET `pWarns` = `pWarns` + '1', `pLeader` = '0', `pMember` = '0', `pRank` = '0', `punWarnstime` = `punWarnstime` + '%i' WHERE `Name` = '%e'",unix + 7*86400,tmp);
mysql_empty(connects, string);
SCMF(playerid, COLOR_WHITE, "Аккаунт %s успешно был предупреждён(warn).",tmp);
format(string,144,"%s получил предупреждение в оффлайне от %s. Причина: %s",tmp,PlayerInfo[playerid][pNames],reason);
SendClientMessageToAll(COLOR_LIGHTRED, string);

gAdmin[playerid][ADMIN_WARN] += 1;
}
else
{
format(string,128, "[OFFWARN] Администратор: %s заблокировал %s [3 предупреждения] на 3 дня. Причина: %s",PlayerInfo[playerid][pNames], tmp, reason);
SendAdminMessage(COLOR_LIGHTRED, string);
format(string,128, "%s заблокирован в Offline администратором: %s [3 предупреждения] на 3 дня. Причина: %s",tmp,PlayerInfo[playerid][pNames],reason);
SendClientMessageToAll(COLOR_LIGHTRED, string);
mysql_format(connects, string, MAX_STR, "UPDATE `accounts` SET `pWarns` = '0', `pLeader` = '0', `pMember` = '0', `pRank` = '0', `punWarnstime` = '0' WHERE `Name` = '%e'",tmp);
mysql_empty(connects, string);
new query_mysql[200], data[16];
new un_ban_date = unix + 3*86400;
format(data,sizeof(data), "%s",date("%dd.%mm.%yyyy",unix));
format(query_mysql,sizeof(query_mysql), "INSERT INTO `BanList` (`Nick`,`Admin`, `Date` ,`Reason`,`Days`,`UnbanDate`,`Status`) VALUES ('%e','%e','%e','%s (3 warns)','3','%d','1')", tmp, PlayerInfo[playerid][pNames], data, reason, un_ban_date);
mysql_empty(connects, query_mysql);

gAdmin[playerid][ADMIN_BAN] += 1;
return 1;
}
return 1;
}
