stock TimeConverter(seconds)
{
new strings[64];
new minutes = floatround(seconds/60);
seconds -= minutes*60;
format(strings, 64, "%02d:%02d", minutes, seconds);
return strings;
}
stock SendBand(playerid=-1,bool:type,one,two,color,str1[])
{
tforeach(x)
{
if(!type && (PlayerInfo[x][pMember] == one || PlayerInfo[x][pMember] == two)) SendClientMessage(x,color,str1);
else if(type && PlayerInfo[x][pMember] == PlayerInfo[playerid][pChs]) SendClientMessage(x,color,str1);
}
return 1;
}
stock GetPlayer2DZone(playerid, zone[], len)
{
new Float:x, Float:y, Float:z;
