stock SpeedPlayer(playerid)
{
new Float:ST[4];
if(!IsPlayerInAnyVehicle(playerid)) GetPlayerVelocity(playerid,ST[0],ST[1],ST[2]);
else GetVehicleVelocity(VehicleEnters[playerid],ST[0],ST[1],ST[2]);
ST[3] = floatsqroot(floatpower(floatabs(ST[0]), 2.0) + floatpower(floatabs(ST[1]), 2.0) + floatpower(floatabs(ST[2]), 2.0)) * 126.31;
return floatround(ST[3]);
}
