else AttachTrailerToVehicle(vid,VehicleEnters[playerid]);
if(GetPVarInt(playerid, "JOB_LSPD_1") == 1 && GetPVarInt(playerid, "JOB_LSPD_CAR") != -1 && vid == Car_V[GetPVarInt(playerid, "JOB_LSPD_CAR")])
