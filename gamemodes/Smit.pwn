CMD:pay(playerid, params[])
{
if(sscanf(params,"dd",params[0],params[1])) return	SendClientMessage(playerid, COLOR_WHITE, ""#INFO"Введите: /pay [id] [сумма]");
if(params[1] > 100000 && PlayerInfo[playerid][pLevel] < 7) return SendClientMessage(playerid, COLOR_GRAD1, "Вы должны быть 7 уровня, чтобы передать 100000 вирт!");
if(params[1] < 1 || params[1] > 100000) return   SendClientMessage(playerid, COLOR_GRAD1, "Нельзя передать меньше $1 и больше $99999.");
if(playerid == params[0]) return 1;
if (!IsPlayerConnected(params[0])) return SendClientMessage(playerid, -1, ""#NET"Человек не найден!");
if(PlayerStartRecon[params[0]]) return SendClientMessage(playerid, -1, ""#NET"Человек слишком далеко!");
