if(PlayerInfo[playerid][pJob] != 1) return SendClientMessage(playerid, -1, ""#NET"Вы не механик!");
if(sscanf(params,"dd",params[0],params[1])) return	SendClientMessage(playerid, COLOR_GRAD2, " Введите: /repair [id] [цена]");
if(params[1] < 1 || params[1] > 1000) return SendClientMessage(playerid, -1, ""#NET"Нельзя меньше 1, и больше 1000!");
if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, -1, ""#NET"Человек не найден!");
if(GetVehicleModel(VehicleEnters[playerid]) != 525) return   SendClientMessage(playerid, -1, ""#NET"Вы не на эвакуаторе!");
if(!ProxDetectorS(8.0, playerid, params[0]) && !IsPlayerInAnyVehicle(params[0])) return SendClientMessage(playerid, -1, ""#NET"Он не в машине!");
if(params[0] == playerid) return SendClientMessage(playerid, -1, ""#NET"Вы не можете чинить машину самому себе!");
string[0] = EOS;
format(string,MAX_STR, "Вы предложили %s отремонтировать его машину за %d вирт.",PlayerInfo[params[0]][pNames],params[1]);
SendClientMessage(playerid, 0x6ab1ffaa, string);
format(string,MAX_STR, "Механик %s предлагает отремонтировать ваш автомобиль за %d вирт.(Введите /accept repair),чтобы совершить починку.",PlayerInfo[playerid][pNames],params[1]);
SendClientMessage(params[0], 0x6ab1ffaa, string);
SetPVarInt(params[0], "RepairOffer", playerid);
SetPVarInt(params[0], "RepairPrice", params[1]);
return 1;
}
CMD:eject(playerid, params[])
{
if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, -1, ""#NET"Вы должны быть в машине!");
if(GetPlayerState(playerid)!=PLAYER_STATE_DRIVER) return SendClientMessage(playerid,-1,""#NET"Вы не водитель!");
if(sscanf(params,"d",params[0])) return	SendClientMessage(playerid, -1, ""#INFO"Используйте /eject [id]");
if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, -1, ""#NET"Человек не найден!");
if(params[0] == playerid) return SendClientMessage(playerid, -1, ""#NET"Вы не можете выкинуть из машины себя!");
if(!IsPlayerInVehicle(params[0],VehicleEnters[playerid])) return SendClientMessage(playerid, -1, ""#NET"Не рядом с вами!");
string[0] = EOS;
if(PlayerCuffed[params[0]] > 0)
{
RemovePlayerFromVehicle(params[0]);
format(string,MAX_STR, "Вы выкинули из машины %s, наручники сломались!", PlayerInfo[params[0]][pNames]);
SendClientMessage(playerid, 0x6495EDFF, string);
format(string,MAX_STR, "Вас выкинул из машины %s, наручники сломались!!", PlayerInfo[playerid][pNames]);
SendClientMessage(params[0], 0x6495EDFF, string);
TogglePlayerControllable(params[0], 1);
SetPlayerSpecialAction(params[0],SPECIAL_ACTION_NONE);
RemovePlayerAttachedObject(params[0],0);
PlayerCuffed[params[0]] = 0;
}
else
{
format(string,MAX_STR, "Вы выкинули из машины %s!", PlayerInfo[params[0]][pNames]);
SendClientMessage(playerid, 0x6495EDFF, string);
format(string,MAX_STR, "Вас выкинул из машины %s!", PlayerInfo[playerid][pNames]);
SendClientMessage(params[0], 0x6495EDFF, string);
RemovePlayerFromVehicle(params[0]);
PlayerTied{params[0]} = false;
TogglePlayerControllable(params[0], 1);
}
return 1;
}
CMD:delveh(playerid)
{
if (PlayerInfo[playerid][bAdmin] < 7) return  SendClientMessage(playerid, -1, ""#NET"Вам недоступна данная функция");
new vehicleid = VehicleEnters[playerid];
if(vehicleid == -1) return SCM(playerid, -1, ""#NET"Вы не в автомобиле!");
if(AllVehicleIsCreated <= 0) return SendClientMessage(playerid, -1, ""#NET"Машины не созданы!");
if(VehicleCreator[vehicleid - 1] != true) return SendClientMessage(playerid, -1, ""#NET"Эту машину удалять нельзя");
RemovePlayerFromVehicle(playerid), VehicleCreator[vehicleid - 1] = false, DestroyVehicle(vehicleid), AllVehicleIsCreated--;
return 1;
}
CMD:delvehall(playerid)
{
if(PlayerInfo[playerid][bAdmin] < 7) return 1;
new VehiclesIsDeleted = 0;
for(new i = 1, j = GetVehiclePoolSize(); i <= j; i++)
{
if(VehicleCreator[i - 1] == false) continue;
VehicleCreator[i - 1] = false;
DestroyVehicle( i );
VehiclesIsDeleted ++;
}
AllVehicleIsCreated=0;
SCMF(playerid, -1, ""#GOOD"Вы удалили %i автомобилей созданных на сервере!", VehiclesIsDeleted);
format(string,128,"%s %s(%d) удалил весь созданный транспорт на сервере. (%i шт)",PlayerAdmin[PlayerInfo[playerid][bAdmin]-1],PlayerInfo[playerid][pNames], playerid, VehiclesIsDeleted);
ABroadCast(COLOR_GREY, string, 1);
return 1;
}
