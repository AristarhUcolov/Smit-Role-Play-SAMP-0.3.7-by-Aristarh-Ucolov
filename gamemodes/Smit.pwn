publics: GzCheck(i,napad,vlad)
{
if(GZInfo[i][ZoneTypeBattle] == 2)
{
CountOnZone[napad] = 0;
CountOnZone[vlad] = 0;
}
tforeach(m)
{
if(PlayerInfo[m][kapture][0] && !IsAGang(m)) EndCapt(m);
else if(PlayerInfo[m][pMember] == napad || PlayerInfo[m][pMember] == vlad)
{
if(PlayerToKvadrat(m,GZInfo[i][gCoords][0],GZInfo[i][gCoords][1],GZInfo[i][gCoords][2],GZInfo[i][gCoords][3]) && GZInfo[i][ZoneTypeBattle] == 2)
{
if(PlayerInfo[m][pMember] == napad) CountOnZone[napad]++;
else if(PlayerInfo[m][pMember] == vlad) CountOnZone[vlad]++;
}
if(!PlayerInfo[m][kapture][0])
{
GangZoneFlashForPlayer(m,GZInfo[i][gID],GetGangColor(napad));
TextDrawShowForPlayer(m, Capture[0]);
TextDrawShowForPlayer(m, Capture[1]);
TextDrawShowForPlayer(m, Capture[2]);
TextDrawSetString(Capture[2],TimeConverter(GZInfo[i][gTime]));
TextDrawSetString(Capture[3],GetGangName(vlad));
TextDrawSetString(Capture[4],GetGangName(napad));
TextDrawSetString(Capture[8],IntToStr(GZInfo[i][ZoneTypeBattle]));
TextDrawShowForPlayer(m, Capture[3]);
TextDrawShowForPlayer(m, Capture[4]);
TextDrawShowForPlayer(m, Capture[5]);
TextDrawShowForPlayer(m, Capture[6]);
TextDrawShowForPlayer(m, Capture[7]);
TextDrawShowForPlayer(m, Capture[8]);

total_myaso[0] = 0, total_myaso[1] = 0;

PlayerInfo[m][kapture][0] = true;
}
else
{
TextDrawSetString(Capture[2],TimeConverter(GZInfo[i][gTime]));
format(string,8,"%d",CountOnZone[vlad]);
TextDrawSetString(Capture[5],string);
format(string,8,"%d",CountOnZone[napad]);
TextDrawSetString(Capture[6],string);
total_myaso[0] = 0, total_myaso[1] = 0;
}
}
}
if(GZInfo[i][gTime]) GZInfo[i][gTime] --;
else if(GZInfo[i][gTime] <= 0)
{
new total[2];
tforeach(m)
{
if(!PlayerToKvadrat(m,GZInfo[i][gCoords][0],GZInfo[i][gCoords][1],GZInfo[i][gCoords][2],GZInfo[i][gCoords][3])) continue;
if(PlayerInfo[m][pMember] == GZInfo[i][gNapad]) total[0]++;
else total[1]++;
}
if(GZInfo[i][ZoneOnBattle] == 1)
{
if(GZInfo[i][ZoneTypeBattle] == 1)
{
string[0] = EOS;
if(!CountOnZone[vlad] && !CountOnZone[napad])
{
if(!total[0] && total[1]) format(string,164,"%s не удалось захватить территорию у банды %s в районе %s",GetGangName(napad),GetGangName(vlad),GZInfo[i][GZName]);
else if(total[0] && !total[1]) format(string,164,"%s захватили территорию у банды %s в районе %s со счётом - 0:0",GetGangName(napad),GetGangName(vlad),GZInfo[i][GZName]), GZInfo[i][gFrakVlad] = GZInfo[i][gNapad], frac_gangzone[GZInfo[i][gNapad]]++;
else if(!total[0] && !total[1]) format(string,164,"%s не удалось захватить территорию у банды %s в районе %s",GetGangName(napad),GetGangName(vlad),GZInfo[i][GZName]);
else if(total[0] && total[1]) return SendBand(-1,false,napad,vlad,CSERVER,"Капт продлен на три минуты в связи с тем, что счёт 0:0 и команды на территории!"), GZInfo[i][gTime] = 180;
SendBand(-1,false,napad,vlad,CSERVER,string);
tforeach(m)
{
if(PlayerInfo[m][pMember] != napad && PlayerInfo[m][pMember] != vlad) continue;
EndCapt(m);
}
CountOnZone[napad] = 0;
CountOnZone[vlad] = 0;
GZInfo[i][ZoneOnBattle] = 0;
GangZoneStopFlashForAll(GZInfo[i][gID]);
GangZoneHideForAll(GZInfo[i][gID]);
GangZoneShowForAll(GZInfo[i][gID],GetGangZoneColor(i));
TextDrawSetString(Capture[5],"0");
TextDrawSetString(Capture[6],"0");
CaptureTime = 20;
CaptureInfo=false;
SaveGZ(i);
KillTimer(GZInfo[i][gzTimer]);
return 1;
}
else if(CountOnZone[vlad] == CountOnZone[napad] || CountOnZone[vlad] > CountOnZone[napad]) format(string,164,"%s захватили территорию у банды %s в районе %s",GetGangName(vlad),GetGangName(napad),GZInfo[i][GZName]);
else
{
format(string,164,"%s захватили территорию у банды %s в районе %s",GetGangName(napad),GetGangName(vlad),GZInfo[i][GZName]);
GZInfo[i][gFrakVlad] = GZInfo[i][gNapad];
frac_gangzone[GZInfo[i][gFrakVlad]]--;
frac_gangzone[GZInfo[i][gNapad]]++;
}
SendBand(-1,false,napad,vlad,CSERVER,string);
GZInfo[i][ZoneOnBattle] = 2;
}
else if(GZInfo[i][ZoneTypeBattle] == 2)
{
if(total[1] >= total[0])
{
SendFamilyMessage(GZInfo[i][gFrakVlad],CSERVER,"[Внимание] Вам удалось отстоять свою территорию.");
SendFamilyMessage(GZInfo[i][gNapad],CSERVER,"[Внимание] Вам не удалось завоевать территорию.");
GZInfo[i][ZoneOnBattle] = 2;
}
else if(total[0] > total[1])
{
format(string,164,"%s захватили территорию у банды %s в районе %s",GetGangName(vlad),GetGangName(napad),GZInfo[i][GZName]);
SendBand(-1,false,napad,vlad,CSERVER,string);
GZInfo[i][gFrakVlad] = GZInfo[i][gNapad];
frac_gangzone[GZInfo[i][gFrakVlad]] --;
frac_gangzone[GZInfo[i][gNapad]] ++;
GZInfo[i][ZoneOnBattle] = 2;
}
}
}
else if(GZInfo[i][ZoneOnBattle] == 2)
{
new string_last[128], best_frag = -1, best_id = -1;
tforeach(m)
{
if(PlayerInfo[m][pMember] == napad || PlayerInfo[m][pMember] == vlad) EndCapt(m);

if(GetPVarInt(m,"killed_capture"))
{
if(GetPVarInt(m,"killed_capture") > best_frag)
{
best_frag = GetPVarInt(m,"killed_capture");
best_id = m;
DeletePVar(m, "killed_capture");
}
}
}
if(best_frag != -1 && best_id != -1) format(string_last, sizeof(string_last), "Лучший игрок на капте "WHITE"%s "SERVER"который уничтожил "WHITE"%d человек(а)", PlayerInfo[best_id][pNames], best_frag);
else if(best_frag == -1) format(string_last, sizeof(string_last), "Лучший игрок во время капта не был определён.");
if(strlen(string_last)) tforeach(m) if(PlayerInfo[m][pMember] == napad || PlayerInfo[m][pMember] == vlad) SendClientMessage(m,CSERVER,string_last);
CountOnZone[napad] = 0;
CountOnZone[vlad] = 0;
GZInfo[i][ZoneOnBattle] = 0;
GZInfo[i][ZoneTypeBattle] = 0;
TextDrawSetString(Capture[5],"0");
TextDrawSetString(Capture[6],"0");
GangZoneStopFlashForAll(GZInfo[i][gID]);
GangZoneHideForAll(GZInfo[i][gID]);
GangZoneShowForAll(GZInfo[i][gID],GetGangZoneColor(i));
CaptureTime = 20;
CaptureInfo = false;
SaveGZ(i);
KillTimer(GZInfo[i][gzTimer]);
return 1;
}
}
return 1;
}
