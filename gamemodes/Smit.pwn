RACE_PLAYERS--;
DeletePVar(playerid, "PlayerInfo_MP"), DeletePVar(playerid, "RACE_POSITION"), DeletePVar(playerid, "RACE_CHECKPOINT");
DestroyVehicle(GetPVarInt(playerid, "RACE_VEHICLE")), DeletePVar(playerid, "RACE_VEHICLE");

SetPlayerPosEx(playerid, GetPVarFloat(playerid,"Pos_X_MP"), GetPVarFloat(playerid,"Pos_Y_MP"), GetPVarFloat(playerid,"Pos_Z_MP"));
SetPlayerFacingAngle(playerid, GetPVarFloat(playerid,"Pos_A_MP"));
SetPlayerVirtualWorld(playerid, GetPVarInt(playerid,"Pos_VW_MP"));
SetPlayerInterior(playerid, GetPVarInt(playerid,"Pos_INT_MP"));

DeletePVar(playerid,"Pos_X_MP");
DeletePVar(playerid,"Pos_Y_MP");
DeletePVar(playerid,"Pos_Z_MP");
DeletePVar(playerid,"Pos_A_MP");
DeletePVar(playerid,"Pos_VW_MP");
DeletePVar(playerid,"Pos_INT_MP");

if(RACE_PLAYERS <= 0)
{
RACE_START = false, RACE_MAP = 0, RACE_WINNERS = 0;
RACE_PLAYERS = 0,RACE_VEHICLE = 0,RACE_TIME_TO_START[0] = 0, RACE_TIME_TO_START[1] = 0,RACE_TIME_TO_FINISH = 0,MAX_RACE_PLAYER = 0;
