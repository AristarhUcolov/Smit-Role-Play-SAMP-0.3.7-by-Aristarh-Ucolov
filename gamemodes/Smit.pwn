stock MeAction(playerid,action[],Float:distance = 13.0)
{
if(GetPVarInt(playerid, "gag")) return	SendClientMessage(playerid, COLOR_LIGHTRED, "У Вас кляп, вы не можете говорить!");
if(PlayerInfo[playerid][pMuteTime]) return SendClientMessage(playerid,COLOR_WHITE,"У Вас бан чата");
string[0] = EOS;
format(string,SMALL_SIZE,"* %s %s",PlayerInfo[playerid][pNames],action);
ProxDetector(distance, playerid, string, 0xDD90FFFF,0xDD90FFFF,0xDD90FFFF,0xDD90FFFF,0xDD90FFFF);
return 1;
}

stock UpdateBizInfo( i )
{
new strin[400];
DestroyDynamicMapIcon(BizInfo[ i ][ bMapIcon ]);
DestroyDynamicPickup(BizInfo[i][bLab]);
DestroyDynamicPickup( BizInfo[ i ][ bPickup ]);
if(BizInfo[i][bTypeBiz] != 9)
{
if(!strcmp(BizInfo[i][bOwner], "None",true))
{
BizInfo[i][ bPickup ] = CreateDynamicPickup(1274, 1, BizInfo[i][EnterBiz][0], BizInfo[i][EnterBiz][1], BizInfo[i][EnterBiz][2],-1,-1,-1,100.0);
format(strin, sizeof(strin), ""SERVER"%s\n"WHITE"Бизнес находится на продаже\nЦена: "SERVER"%i$", BizInfo[i][bName],BizInfo[i][bPrice]);
BizInfo[i][bLab] = CreateDynamicPickup(1239,1,BizInfo[ i ][BarPos][0], BizInfo[ i ][BarPos][1], BizInfo[ i ][BarPos][2],BizInfo[ i ][ bVW ],BizInfo[ i ][ bInt ],-1,100.0);
}
else
{
BizInfo[i][ bPickup ] = CreateDynamicPickup(19132, 1, BizInfo[i][EnterBiz][0], BizInfo[i][EnterBiz][1], BizInfo[i][EnterBiz][2],-1,-1,-1,100.0);
if(BizInfo[i][bVhod] > 0) format(strin, sizeof(strin), ""SERVER"%s\n"WHITE"Владелец:"SERVER" %s\n"WHITE"Цена за вход: "SERVER"$%i\n"WHITE"Контроль: "SERVER"%s", BizInfo[i][bName],BizInfo[i][bOwner],BizInfo[i][bVhod],GetMN(BizInfo[i][bKrisha]));
else format(strin, sizeof(strin), ""SERVER"%s\n"WHITE"Владелец: "SERVER"%s\n"WHITE"Контроль:"SERVER" %s", BizInfo[i][bName],BizInfo[i][bOwner],GetMN(BizInfo[i][bKrisha]));
}
}
else
{
BizInfo[i][ bPickup ] = CreateDynamicPickup(19132, 1, BizInfo[i][EnterBiz][0], BizInfo[i][EnterBiz][1], BizInfo[i][EnterBiz][2],-1,-1,-1,100.0);
format(strin, sizeof strin, ""SERVER"Нарко притон\n"WHITE"Владелец: {"#cGREEN"}%s", GetMN(BizInfo[i][bKrisha]));
}
BizInfo[i ][ bMapIcon ] = CreateDynamicMapIcon(BizInfo[i][EnterBiz][0],BizInfo[ i ][EnterBiz][1],BizInfo[ i ][EnterBiz][2], 56, -1 );
return UpdateDynamic3DTextLabelText( BizInfo[ i ][bLabel ], 0xC4DAADFF, strin);
}

