publics:_@players_inviters(playerid,page)
{
new rows,fields;
cache_get_data(rows,fields,connects);
if(!rows) return SendClientMessage(playerid, -1, ""#NET"Никто не хочет вступить в Вашу организацию!"), DeletePVar(playerid, "rinvite");
if(GetPVarInt(playerid, "cFractionList"))
{
for(new i; i < 15; i ++) StringName[playerid][i][0] = EOS;
DeletePVar(playerid, "cFractionList");
}
new Name[24], string_last[64], t;
string[0] = EOS;
format(string, sizeof(string), ""WHITE"Имя Фамилия\n"SERVER"Перейти на следующую страницу\nПерейти на предыдущую страницу\n");
for(new i = (page != 1 ? (page*15) : (0)); i < rows; i ++)
{
if((rows>15 && t>=15) || (t>=rows))break;
cache_get_field_content(i, "Name", Name, connects, sizeof(Name));
strmid(StringName[playerid][GetPVarInt(playerid, "cFractionList")], Name, 0, strlen(Name), 255);
SetPVarInt(playerid, "cFractionList", GetPVarInt(playerid, "cFractionList")+1);
format(string, sizeof(string), "%s%i."WHITE" Заявление от "SERVER"%s"WHITE"\n", string, (i<15 ? (i+1) : (i-14)), Name);
t++;
}
if(!t) return SendClientMessage(playerid, -1, ""#NET"Нет страниц!"), DeletePVar(playerid, "rinvite"), cmd::rinvite(playerid);
SetPVarInt(playerid, "cList", (page != 1 ? (page) : (1)));
format(string_last, 64, ""SERVER"Страница номер: %i", GetPVarInt(playerid, "cList"));
SPD(playerid, 8957, DIALOG_STYLE_TABLIST_HEADERS, string_last, string, "Далее", "Отмена");
return 1;
}
