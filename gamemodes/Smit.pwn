for(new i; i < 13; i++) TextDrawHideForPlayer(playerid, CounterSText[i]);
if(GetPVarInt(playerid,"PlayerInfo_MP_Staus") == win_command) Hook_GivePlayerMoney(playerid, 1000000), SendClientMessage(playerid, -1, "Поздравляем Вас с победой. Ваш приз выдан! [+1000000$]");
DeletePVar(playerid,"PlayerInfo_MP");
