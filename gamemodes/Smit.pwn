stock IsVehicleDrivingBackwards(vehicleid) {
new Float:Float[3];
if(GetVehicleVelocity(vehicleid, Float[1], Float[2], Float[0])) {
GetVehicleZAngle(vehicleid, Float[0]);
if(Float[0] < 90) {
if(Float[1] > 0 && Float[2] < 0) return 1;
}
else if(Float[0] < 180) {
if(Float[1] > 0 && Float[2] > 0) return 1;
}
else if(Float[0] < 270) {
if(Float[1] < 0 && Float[2] > 0) return 1;
}
else if(Float[1] < 0 && Float[2] < 0) return 1;
}
return 0;
}

