stock Start_Race_MP()
{
if(RACE_TIME_TO_START[1]) RACE_TIME_TO_START[1]--;

if(RACE_TIME_TO_START[1] == 1)
{
SendClientMessageToAll(CYELLOW,"Информация: Гоночные мероприятия начались.");
RACE_START = true, RACE_TIME_TO_FINISH = unix+600;
tforeach(i)
{
if(GetPVarInt(i, "PlayerInfo_MP") != MP_Racing) continue;

GameTextForPlayer(i, "~p~START~n~~g~GO GO GO", 3000, 4);
TogglePlayerControllable(i, true);
SetPlayerRaceCheckpoint(i, 0, RaceCheckPoint[RACE_MAP][0][0], RaceCheckPoint[RACE_MAP][0][1], RaceCheckPoint[RACE_MAP][0][2], RaceCheckPoint[RACE_MAP][0+1][0], RaceCheckPoint[RACE_MAP][0+1][1], RaceCheckPoint[RACE_MAP][0+1][2], 10.0);
}
RACE_TIME_TO_START[1] = 0;
return 1;
}

new str_race[10];
tforeach(i)
{
if(GetPVarInt(i, "PlayerInfo_MP") != MP_Racing) continue;

if(RACE_TIME_TO_START[1] <= 10)
{
format(str_race, 10, "~g~%i", RACE_TIME_TO_START[1]);
GameTextForPlayer(i, str_race, 1000, 4);
}

if(RACE_TIME_TO_START[1] == 5)
{
new Float:Pos_X,Float:Pos_Y,Float:Pos_Z,Float:Pos_A,engine,lights,alarm,doors,bonnet,boot,objective;
GetPlayerPos(i,Pos_X,Pos_Y,Pos_Z);
GetPlayerFacingAngle(i,Pos_A);
SetPVarFloat(i,"Pos_X_MP",Pos_X);
SetPVarFloat(i,"Pos_Y_MP",Pos_Y);
SetPVarFloat(i,"Pos_Z_MP",Pos_Z);
SetPVarFloat(i,"Pos_A_MP",Pos_A);
SetPVarInt(i,"Pos_VW_MP", GetPlayerVirtualWorld(i));
SetPVarInt(i,"Pos_INT_MP",GetPlayerInterior(i));
DisablePlayerRaceCheckpoint(i);
TogglePlayerControllable(i, false);
new POSITION = GetPVarInt(i, "RACE_POSITION");
SetPlayerPosEx(i, RaceVehicleRespawn[RACE_MAP][POSITION][0], RaceVehicleRespawn[RACE_MAP][POSITION][1], RaceVehicleRespawn[RACE_MAP][POSITION][2]);
SetPVarInt(i,"RACE_VEHICLE",_:Forveh_AddStaticVehicle(RACE_VEHICLE,RaceVehicleRespawn[RACE_MAP][POSITION][0],RaceVehicleRespawn[RACE_MAP][POSITION][1],RaceVehicleRespawn[RACE_MAP][POSITION][2],RaceVehicleRespawn[RACE_MAP][POSITION][3],0,0));
LinkVehicleToInterior(GetPVarInt(i,"RACE_VEHICLE"),0);
SetVehicleVirtualWorld(GetPVarInt(i,"RACE_VEHICLE"),1);
VehInfo[GetPVarInt(i,"RACE_VEHICLE")][vFuel] = 100.0;
SetPlayerInterior(i,0);
SetPlayerVirtualWorld(i,1);
PutPlayerInVeh(i,GetPVarInt(i,"RACE_VEHICLE"), 0);
GetVehicleParamsEx(GetPVarInt(i,"RACE_VEHICLE"),engine,lights,alarm,doors,bonnet,boot,objective);
SetVehicleParamsEx(GetPVarInt(i,"RACE_VEHICLE"),true,true,alarm,doors,bonnet,boot,objective);
for(new a;a<sizeof(RTD_box);a++) TextDrawShowForPlayer(i,RTD_box[a]);
PlayerTextDrawShow(i,RTD_MText);
for(new a; a < sizeof(RTD_MName); a++) PlayerTextDrawShow(i,RTD_MName[a]);
PlayerTextDrawShow(i,RTD_Time);
PlayerTextDrawShow(i,RTD_CP);
SetPVarInt(i, "UpdateRacing", _:SetTimerEx("UpdateRaceTD", 1000, true, "d", i));
}
}
return 1;
}
