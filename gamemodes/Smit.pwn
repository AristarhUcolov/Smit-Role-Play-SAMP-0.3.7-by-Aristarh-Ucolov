public OnVehicleSpawn(vehicleid)
{
if(RentCar[vehicleid] != -1)
{
RentCar[vehicleid] = -1;
DeletePVar(RentCar[vehicleid], "VehicleRent");
VehInfo[vehicleid-1][vLock] = false;
SetVehicleParamsForPlayer(vehicleid,RentCar[vehicleid],0,false);
SendClientMessage(RentCar[vehicleid],COLOR_GREY,"Ваш арендованный автомобиль был заспавнен. {ff0000}Аренда окончена");
}
UpdateVehiclePos(-1, vehicleid, 0);
tforeach(i)
{
if(GetPVarInt(i,"prod_vehicle_id") == vehicleid)
{
if(gPlayerProdText[i][0] != Text3D:-1 || gPlayerProdText[i][1] != Text3D:-1 || gPlayerProdCP[i] != -1)
{
if(gPlayerProdText[i][0] != Text3D:-1) DestroyDynamic3DTextLabel(gPlayerProdText[i][0]);
if(gPlayerProdText[i][1] != Text3D:-1) DestroyDynamic3DTextLabel(gPlayerProdText[i][1]);
if(gPlayerProdCP[i] != -1) DestroyDynamicCP(gPlayerProdCP[i]);

gPlayerProdText[i][0] = Text3D:-1;
gPlayerProdText[i][1] = Text3D:-1;
gPlayerProdCP[i] = -1;

SendClientMessage(i, CGRAY, "Ваш рабочий транспорт отбуксирован");
SendClientMessage(i, 0xFFFF00AA, "Рабочий день завершен");

DeletePVar(i,"prod_vehicle_id");
DeletePVar(i,"prod_next");
DeletePVar(i,"count_prod2");
PlayerProdCar[i]=0;
}
}
if(vehicleid == caridhouse[i][0]) LoadVehicleToHouse(i, 1);
else if(vehicleid == caridhouse[i][2]) LoadVehicleToHouse(i, 2);
else if(vehicleid == caridhouse[i][3]) LoadVehicleToHouse(i, 3);
if(PlayerInfo[i][pAvia] != -1 && vehicleid == caridhouse[i][1]) DestroyVehicle(vehicleid), caridhouse[i][1]=0;
if(PlayerRentTook[i] == vehicleid && PlayerRentTookCheck[i])
{
DestroyVehicle(vehicleid);
PlayerRentTook[i]=0;
PlayerRentTookCheck[i]=false;
SendClientMessage(i,COLOR_GREY,"Ваш арендованный транспорт был отбуксирован!");
break;
}
}
if(gVehicleGun[vehicleid][vGunLoading] || gVehicleGun[vehicleid][vGunUnloading])
{
gVehicleGun[vehicleid][vGunGunAmount] = 0;
gVehicleGun[vehicleid][vGunLoading] = false;
gVehicleGun[vehicleid][vGunUnloading] = false;
if(gVehicleGun[vehicleid][vGunText] != Text3D:INVALID_3DTEXT_ID) DestroyDynamic3DTextLabel(gVehicleGun[vehicleid][vGunText]);
gVehicleGun[vehicleid][vGunText] = Text3D:INVALID_3DTEXT_ID;
DestroyDynamicArea(gVehicleGun[vehicleid][vGunArea]);
DestroyDynamicPickup(gVehicleGun[vehicleid][vGunPickup]);
gVehicleGun[vehicleid][vGunArea] = 0;
gVehicleGun[vehicleid][vGunPickup] = 0;
Itter_Remove(VehicleGun,vehicleid);
}
if(Start{vehicleid})
{
DestroyDynamic3DTextLabel(mats3Dtxt[vehicleid]);
DestroyDynamicPickup(autopick_mats[vehicleid]);
Start{vehicleid} = false;
}
if(GetVehicleModel(vehicleid)==426 && vehicleid >= liccar[0] && vehicleid <= liccar[1])
{
DestroyDynamicObject(Automobile[vehicleid][0]);
