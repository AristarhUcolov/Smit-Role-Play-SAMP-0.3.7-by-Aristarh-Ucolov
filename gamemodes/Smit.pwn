publics: LoginCallback(playerid)
{
new rows,fields;
cache_get_data(rows,fields,connects);
if(!rows)
{
if(PlayerPassword[playerid] == 2) return SendClientMessage(playerid,COLOR_GREY,"Вы кикнуты с сервера за неверный ввод пароля. Пожалуйста попробуйте позже!"), KickEx(playerid);
PlayerPassword[playerid] ++;
LoginDialog(playerid);
return 1;
}
if(PlayerInfo[playerid][pLoadNames] != PlayerInfo[playerid][pNames])
{
SendClientMessage(playerid, COLOR_REDD, "Уважаемый игрок! В данный момент некие проблемы с сервером!");
SendClientMessage(playerid, COLOR_REDD, "База данных перегружена и не может обработать Ваш аккаунт!");
SendClientMessage(playerid, COLOR_REDD, "Зайдите через некоторое время (через 2-5 минут).");
SendClientMessage(playerid, COLOR_REDD, "Мы делаем все, чтобы защитить Ваш аккаунт от слета!");
SendClientMessage(playerid, COLOR_REDD, "По возможности отправьте скриншот с этой записью в раздел Баги/Ошибки.");
SCMF(playerid, COLOR_GREEN, "Connect Name: %s | Load Name: %s | ID: %i", PlayerInfo[playerid][pNames], PlayerInfo[playerid][pLoadNames], PlayerInfo[playerid][pID]);
return KickEx(playerid);
}
printf("[RegCheck] %s > %s", PlayerInfo[playerid][pNames], PlayerInfo[playerid][pLoadNames]);
PlayerInfo[playerid][pLogin] = true;
if(GetPVarInt(playerid, "reg_check")) PlayerInfo[playerid][pID] = cache_get_field_content_int(0, "id", connects);
SCMF(playerid, -1, "Ваш аккаунт успешно загружен (%i)", PlayerInfo[playerid][pID]);
TogglePlayerSpectating(playerid, false);
cache_get_field_content(0, "pKey", PlayerInfo[playerid][pKey], connects, 30);
PlayerInfo[playerid][pLevel] = cache_get_field_content_int(0, "pLevel", connects);
