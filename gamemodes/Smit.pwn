publics:OffBan(playerid,NAME[],DAYS,REASON[])
{
new rows,fields;
cache_get_data(rows,fields,connects);
if(rows) return SendClientMessage(playerid, -1, ""#NET"Игрок уже заблокирован!");
if(GetPlayerID(NAME) != -1) return SendClientMessage(playerid, -1, ""#NET"Игрок онлайн");
new query_mysql[200], data[16], un_ban_date = unix + DAYS*86400;
format(data,sizeof(data), "%s",date("%dd.%mm.%yyyy",unix));
mysql_format(connects,query_mysql,sizeof(query_mysql), "INSERT INTO `BanList` (`Nick`,`Admin`, `Date` ,`Reason`,`Days`,`UnbanDate`,`Status`) VALUES ('%e','%e','%e','%e','%i','%d','1')", NAME, PlayerInfo[playerid][pNames], data, REASON, DAYS, un_ban_date);
mysql_empty(connects, query_mysql);
format(string,164,"%s получил оффлайн блокировку от администратора: %s на %i дней. Причина: %s",NAME,PlayerInfo[playerid][pNames],DAYS,REASON);
SendClientMessageToAll(COLOR_LIGHTRED, string);
gAdmin[playerid][ADMIN_BAN] += 1;
return 1;
}
