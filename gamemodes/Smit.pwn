publics: Moderator(playerid,giveplayerid[],level)
{
new statics, rows,fields;
cache_get_data(rows,fields,connects);
if(rows)
{
statics = cache_get_field_content_int(0, "level", connects);
if(!level)
{
mysql_format(connects,string, MAX_STR, "DELETE FROM `admin` WHERE `name`='%e'", giveplayerid);
mysql_empty(connects, string);
mysql_format(connects,string, MAX_STR, "UPDATE `accounts` SET `bAdmin` = '0' WHERE `Name` = '%e'",giveplayerid);
mysql_empty(connects, string);
SCMF(playerid, COLOR_LIGHTRED, "%s успешно удален из списка модераторов", giveplayerid);
}
else
{
if(level > statics && (!strcmp(giveplayerid, FD1, true)&& !strcmp(giveplayerid, FD7, true) && !strcmp(giveplayerid, FD2, true) && !strcmp(giveplayerid, FD3, true))) return SendClientMessage(playerid, -1, ""#NET"Вы не можете повышать!");
mysql_format(connects,string, MAX_STR, "UPDATE `admin` SET  `level` = '%d' WHERE `name` = '%e' LIMIT 1", level,giveplayerid);
mysql_empty(connects, string);
SCMF(playerid, COLOR_GREEN, "Уровень администратирования у %s изменен на %d", giveplayerid, level);
}
return 1;
}
if(!level) return SendClientMessage(playerid,-1,""#NET"Игрок не администратор.");
new years, months, days,data[32]; r_getdate(years, months, days);
format(data, 32, "%02d.%02d.%04d", days, months, years);
mysql_format(connects,string, 246, "INSERT INTO `admin` (`Name`, `level`, `LastCon`, `Postavil`, `Data`) VALUES ('%e', %d, '%e', '%e', '%e')", giveplayerid, level, data, PlayerInfo[playerid][pNames], data);
