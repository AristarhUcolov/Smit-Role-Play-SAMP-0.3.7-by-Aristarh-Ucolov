{
GameTextForPlayer(playerid,"~w~Auto authorization ~n~~p~please wait...",6000,4);

cache_get_field_content(0,"pKey",PlayerInfo[playerid][pKey],connects,32);

strmid(PlayerInfo[playerid][pLoadNames], PlayerInfo[playerid][pNames], 0, 24, 255);

SetTimerEx("PlayerAuto_Authorization", 3500, false, "d", playerid);

return 1;
}
else
{
