if(PlayerInfo[playerid][pMember]) SetPlayerSkinEx(playerid,PlayerInfo[playerid][pModel]);
else SetPlayerSkinEx(playerid,PlayerInfo[playerid][pChar]);
SetPlayerColor(playerid, FractionInfo[PlayerInfo[playerid][pMember]][fColor]);
}
