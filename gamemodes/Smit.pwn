public OnUnoccupiedVehicleUpdate(vehicleid, playerid, passenger_seat, Float:new_x, Float:new_y, Float:new_z, Float:vel_x, Float:vel_y, Float:vel_z)
{
new Float:Pos[3];
GetVehiclePos(vehicleid, Pos[0], Pos[1], Pos[2]);

for(new i; i < MAX_SELL; i ++)
{
if(SellingCar[i][sellVehicle] != -1 && vehicleid == SellingCar[i][sellVehicle] && (Pos[0] != CoordSellCars[i][0] && Pos[1] != CoordSellCars[i][1] && Pos[2] != CoordSellCars[i][2]))
{
SetVehiclePos(vehicleid, CoordSellCars[i][0], CoordSellCars[i][1], CoordSellCars[i][2]);
break;
}
}

new Float:Count[2];
Count[0] = Difference(Pos[0],VehInfo[vehicleid - 1][vehicleX]);
Count[1] = Difference(Pos[1],VehInfo[vehicleid - 1][vehicleY]);
switch(GetVehicleModel(vehicleid))
{
case 435, 450, 584, 591, 606..608, 610..611: goto UPDATE;
}
new bool:check__voditel = false;
tforeach(i)
{
if(VehicleEnters[i] == vehicleid)
{
check__voditel = true;
break;
}
}
if((Count[0] > 5 || Count[1] > 5) && !check__voditel && !BanCar[vehicleid])
{
SetVehiclePos(vehicleid, VehInfo[vehicleid - 1][vehicleX], VehInfo[vehicleid - 1][vehicleY], VehInfo[vehicleid - 1][vehicleZ]);
SetVehicleZAngle(vehicleid, VehInfo[vehicleid - 1][vehicleA]);
}
else UPDATE: UpdateVehiclePos(playerid, vehicleid, 0);
return 1;
}
