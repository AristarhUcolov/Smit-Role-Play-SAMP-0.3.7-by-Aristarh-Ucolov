publics: UpdateSpeedometr(playerid)
{
if(!PlayerShowSpeed[playerid] || GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return 1;
if(!pTemp[playerid][pAFK])
{
new str_speed[70];
new vehicleid = VehicleEnters[playerid];

if(vehicleid <= 0) return 0;

if(VehInfo[vehicleid][vFuel] <= 1.0)
{
SendClientMessage(playerid,-1,""#NET"В машине нет бензина");
new engine,lights,alarm,doors,bonnet,boot,objective;
GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
SetVehicleParamsEx(vehicleid, false, lights, alarm, doors, bonnet, boot, objective);

return 0;
}

new Float:X2, Float:Y2, Float:Z2,Float:X,Float:Y,Float:Z;

X2 = VehInfo[vehicleid-1][vehicleX];
Y2 = VehInfo[vehicleid-1][vehicleY];
Z2 = VehInfo[vehicleid-1][vehicleZ];

GetVehicleVelocity(vehicleid,X,Y,Z);

new Float:speed = floatsqroot(floatadd(floatadd(floatpower(X, 2), floatpower(Y, 2)),  floatpower(Z, 2))) * 126.31;

new PlayerSpeed = floatround(speed);

if(GetPVarInt(playerid, "slimit") && PlayerSpeed >= GetPVarInt(playerid,"slimit"))
{
new Float:dif = GetPVarInt(playerid,"slimit") / speed;
SetVehicleVelocity(vehicleid,X*dif,Y*dif,Z);
}

GetVehiclePos(vehicleid, X, Y, Z);

//
VehInfo[vehicleid-1][vehicleX] = X;
VehInfo[vehicleid-1][vehicleY] = Y;
VehInfo[vehicleid-1][vehicleZ] = Z;
//
new Float:veloc = PointToPoint3D(X2,Y2,Z2,X,Y,Z)/1000;
new Float:health;
GetVehicleHealth(vehicleid,health);

VehInfo[vehicleid-1][vDrived] += veloc;

format(str_speed, sizeof(str_speed), "%06.0f", VehInfo[vehicleid-1][vDrived]);
PlayerTextDrawSetString(playerid, Speedometr[8][playerid], str_speed);

if(VehInfo[vehicleid][vFuel] > 1.0 && !IsAPlane(GetVehicleModel(vehicleid))) VehInfo[vehicleid][vFuel] -= (veloc*2);
else if(IsAPlane(GetVehicleModel(vehicleid))) VehInfo[vehicleid][vFuel] -= 0.001;

format(str_speed, sizeof(str_speed), "%d", floatround(health, floatround_tozero));
PlayerTextDrawSetString(playerid, Speedometr[2][playerid], str_speed); // ХП
format(str_speed, sizeof(str_speed), "%d", floatround(VehInfo[vehicleid][vFuel], floatround_tozero));
PlayerTextDrawSetString(playerid, Speedometr[5][playerid], str_speed); // FUEL
PlayerTextDrawSetString(playerid, Speedometr[6][playerid], GetPVarInt(playerid, "playerisGPS") ? ("ON") : ("OFF")); // GPS
if(PlayerSpeed)
{
str_speed[0]=EOS;
for(new dspeed; dspeed < floatround((17.0/200.0)*float(PlayerSpeed)) && dspeed<15; dspeed++)
{
switch(dspeed)
{
case 5: format(str_speed, sizeof(str_speed), "%s~b~", str_speed);
case 10: format(str_speed, sizeof(str_speed), "%s~r~", str_speed);
}
format(str_speed, sizeof(str_speed), "%sI", str_speed);
}
PlayerTextDrawSetString(playerid, Speedometr[0][playerid], str_speed);
format(str_speed, sizeof(str_speed), "%i", PlayerSpeed);
PlayerTextDrawSetString(playerid, Speedometr[7][playerid], str_speed);
}
else
{
PlayerTextDrawSetString(playerid, Speedometr[0][playerid], "_");
PlayerTextDrawSetString(playerid, Speedometr[7][playerid], "0");
}
}
SetTimerEx("UpdateSpeedometr",50,0,"i",playerid);
return 1;
}
