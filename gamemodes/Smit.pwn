if(PlayerTied{playerid} > false)  return SendClientMessage(playerid, -1, ""#NET"Вы связаны!");
if(PlayerTied{params[0]} > false) return SendClientMessage(playerid, -1, ""#NET"Человек уже связан!");
if(params[0] == playerid) return SendClientMessage(playerid, -1, ""#NET"Нельзя связать самого себя!");
if (!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_WHITE, ""#NET"Человек не рядом с вами!");
